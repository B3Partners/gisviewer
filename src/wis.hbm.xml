<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="nl.b3p.nbr.wis.db.Applicaties">
        <id name="id" type="int">
            <column name="id" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="pakket"/>
        <property name="module"/>
        <property name="beschrijving"/>
        <property name="extern" not-null="true"/>
        <property name="acceptabel" not-null="true"/>
        <property name="administratief" not-null="true"/>
        <property name="geodata" not-null="true"/>
        <property name="taal"/>
        <property name="spatialKoppeling"/>
        <property name="dbKoppeling"/>
        <property name="webbased" not-null="true"/>
        <property name="gps" not-null="true"/>
        <property name="crow" not-null="true"/>
        <property name="opmerking"/>
        <many-to-one lazy="false" name="leverancier"/>
    </class>
    <class name="nl.b3p.nbr.wis.db.Clusters">
        <id name="id" type="int">
            <column name="id" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="naam"/>
        <property name="omschrijving"/>
        <many-to-one name="parent" column="parent"/>
        <set name="children" inverse="true" cascade="all-delete-orphan">
            <key column="parent"/>
            <one-to-many class="nl.b3p.nbr.wis.db.Clusters"/>
        </set>
        <set name="themas" cascade="all-delete-orphan">
            <key column="cluster"/>
            <one-to-many class="nl.b3p.nbr.wis.db.Themas"/>
        </set>
    </class>
    <class name="nl.b3p.nbr.wis.db.DataTypen">
        <id name="id" type="int">
            <column name="id" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="naam"/>
    </class>
    <class name="nl.b3p.nbr.wis.db.FunctieItems">
        <id name="id" type="int">
            <column name="id" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="label"/>
        <property name="omschrijving"/>
        <property name="eenheid"/>
        <property name="voorbeelden"/>
        <property name="invoer" not-null="true"/>
        <property name="uitvoer" not-null="true"/>
        <many-to-one name="functie"/>
    </class>
    <class name="nl.b3p.nbr.wis.db.Leveranciers">
        <id name="id" type="int">
            <column name="id" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="naam"/>
        <property name="pakket"/>
        <property name="telefoon1"/>
        <property name="contact"/>
        <property name="telefoon2"/>
        <property name="telefoon3"/>
        <property name="email"/>
        <property name="info" not-null="true"/>
        <property name="opmerkingen"/>
    </class>
    <class name="nl.b3p.nbr.wis.db.LocatieAanduidingen">
        <id name="id" type="int">
            <column name="id" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="naam"/>
        <property name="omschrijving"/>
    </class>
    <class name="nl.b3p.nbr.wis.db.Medewerkers">
        <id name="id" type="int">
            <column name="id" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="Achternaam"/>
        <property name="Voornaam"/>
        <property name="telefoon"/>
        <property name="Functie"/>
        <property name="Locatie"/>
        <property name="email"/>
    </class>
    <class name="nl.b3p.nbr.wis.db.Moscow">
        <id name="id" type="int">
            <column name="id" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="code"/>
        <property name="naam"/>
        <property name="omschrijving"/>
    </class>
    <class name="nl.b3p.nbr.wis.db.Onderdeel">
        <id name="id" type="int">
            <column name="id" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="naam"/>
        <property name="omschrijving"/>
        <property name="locatie"/>
        <property name="regio" not-null="true"/>
    </class>
    <class name="nl.b3p.nbr.wis.db.OnderdeelMedewerkers">
        <id name="id" type="int">
            <column name="id" not-null="true"/>
            <generator class="identity"/>
        </id>
        <many-to-one name="medewerker"/>
        <many-to-one name="onderdeel"/>
        <property name="vertegenwoordiger" not-null="true"/>
    </class>
    <class name="nl.b3p.nbr.wis.db.Rollen">
        <id name="id" type="int">
            <column name="id" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="naam"/>
    </class>
    <class name="nl.b3p.nbr.wis.db.ThemaApplicaties">
        <id name="id" type="int">
            <column name="id" not-null="true"/>
            <generator class="identity"/>
        </id>
        <many-to-one name="thema"/>
        <many-to-one lazy="false" name="applicatie"/>
        <property name="ingebruik" not-null="true"/>
        <property name="geodata" not-null="true"/>
        <property name="administratief" not-null="true"/>
        <property name="voorkeur" not-null="true"/>
        <property name="definitief" not-null="true"/>
        <property name="standaard" not-null="true"/>
    </class>
    <class name="nl.b3p.nbr.wis.db.ThemaFuncties">
        <id name="id" type="int">
            <column name="id" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="naam"/>
        <property name="omschrijving"/>
        <many-to-one name="thema"/>
        <many-to-one name="applicatie" lazy="false"/>
        <property name="protocol"/>
    </class>
    <class name="nl.b3p.nbr.wis.db.ThemaData">
        <id name="id" type="int">
            <column name="id" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="label"/>
        <property name="eenheid"/>
        <property name="omschrijving"/>
        <property name="basisregel" not-null="true"/>
        <property name="voorbeelden"/>
        <property name="kolombreedte" not-null="true"/>
        <many-to-one name="thema" lazy="false" />
        <many-to-one name="moscow" lazy="false" />
        <many-to-one name="waardeType" lazy="false" />
        <many-to-one name="dataType" lazy="false" />
        <property name="commando"/>
        <property name="kolomnaam"/>
        <property name="dataorder"/>
    </class>
    <class name="nl.b3p.nbr.wis.db.ThemaVerantwoordelijkheden">
        <id name="id" type="int">
            <column name="id" not-null="true"/>
            <generator class="identity"/>
        </id>
        <many-to-one name="thema"/>
        <many-to-one name="medewerker" lazy="false"/>
        <many-to-one name="onderdeel" lazy="false"/>
        <many-to-one name="rol" lazy="false"/>
        <property name="huidige_situatie" not-null="true"/>
        <property name="gewenste_situatie" not-null="true"/>
        <property name="opmerkingen"/>
    </class>
    <class name="nl.b3p.nbr.wis.db.Themas">
        <id name="id" type="int">
            <column name="id" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="code"/>
        <property name="naam"/>
        <many-to-one name="moscow" lazy="false"/>
        <property name="belangnr" not-null="true"/>
        <many-to-one name="cluster" lazy="false">
            <column name="cluster" not-null="true"/>
        </many-to-one>
        <property name="opmerkingen"/>
        <property name="analyse_thema" not-null="true"/>
        <property name="locatie_thema" not-null="true"/>
        <property name="admin_tabel_opmerkingen"/>
        <property name="admin_tabel"/>
        <property name="admin_pk"/>
        <property name="admin_pk_complex" not-null="true"/>
        <property name="admin_spatial_ref"/>
        <property name="admin_query"/>
        <property name="spatial_tabel_opmerkingen"/>
        <property name="spatial_tabel"/>
        <property name="spatial_pk"/>
        <property name="spatial_pk_complex" not-null="true"/>
        <property name="spatial_admin_ref"/>
        <property name="wms_url"/>
        <property name="wms_layers"/>
        <property name="wms_layers_real"/>
        <property name="wms_legendlayer"/>
        <property name="wms_legendlayer_real"/>
        <property name="wms_querylayers"/>
        <property name="wms_querylayers_real"/>
        <property name="update_frequentie_in_dagen"/>
        <set name="themaData" lazy="false" cascade="all-delete-orphan" order-by="dataorder, basisregel desc, label">
            <key column="thema"/>
            <one-to-many class="nl.b3p.nbr.wis.db.ThemaData"/>
        </set>
        <set name="themaVerantwoordelijkheden" lazy="false" cascade="all-delete-orphan" order-by="rol, huidige_situatie desc">
            <key column="thema"/>
            <one-to-many class="nl.b3p.nbr.wis.db.ThemaVerantwoordelijkheden"/>
        </set>
        <set name="themaApplicaties" lazy="false" cascade="all-delete-orphan" order-by="definitief desc, voorkeur desc">
            <key column="thema"/>
            <one-to-many class="nl.b3p.nbr.wis.db.ThemaApplicaties"/>
        </set>
    </class>
    <class name="nl.b3p.nbr.wis.db.WaardeTypen">
        <id name="id" type="int">
            <column name="id" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="naam"/>
    </class>
    <class name="nl.b3p.nbr.wis.db.WorkshopMedewerkers">
        <id name="id" type="int">
            <column name="id" not-null="true"/>
            <generator class="identity"/>
        </id>
        <many-to-one name="workshop"/>
        <many-to-one name="medewerker"/>
        <property name="aanwezig" not-null="true"/>
    </class>
    <class name="nl.b3p.nbr.wis.db.Workshops">
        <id name="id" type="int">
            <column name="id" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="volgnr" not-null="true"/>
        <property name="naam"/>
    </class>
</hibernate-mapping>
