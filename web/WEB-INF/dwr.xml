<!DOCTYPE dwr PUBLIC
    "-//GetAhead Limited//DTD Direct Web Remoting 1.0//EN"
    "http://www.getahead.ltd.uk/dwr/dwr10.dtd">
<dwr>
    <allow>
        <create creator="new" javascript="JMapData">
            <param name="class" value="nl.b3p.gis.viewer.GetLocationData"/>
        </create>
        <convert converter="bean" match="nl.b3p.gis.viewer.MapCoordsBean"/>
        <create creator="new" javascript="JConfigListsUtil">
            <param name="class" value="nl.b3p.gis.viewer.ConfigListsUtil"/>
            <include method="getPossibleFeaturesById"/>
            <include method="getPossibleAttributesById"/>
        </create>
    </allow>
</dwr>