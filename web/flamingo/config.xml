<FLAMINGO xmlns:fmc="fmc" id="flamingo" lang="nl" languages="en,nl" tooltipdelay="300" commtojava="true" preloadtitle="Prototypemap" useexternalinterface="true">
    <style id=".tooltip" font-family="arial, sans-serif" font-size="11" color="#880000" display="block"/>
    <yguide id="my" y="50%"/>
    <xguide id="mx" x="50%"/>
    <fmc:MonitorLayer id="monitorl" left="37%" top="50%" width="50%" listento="map1">
        <string id="waiting"  en="Making... [layer] [percentage]%..." nl="Kaart aan het maken..."/>
        <string id="loading"  en="Making... [layer] [percentage]%..." nl="Kaart aan het laden. [percentage]%"/>
    </fmc:MonitorLayer>
    <fmc:Container backgroundcolor="#D2E0EB" left="0" top="0" height="6%" width="100%">
        <fmc:ToolGroup tool="zoom" listento="map1" width="148" height="65" left="0" top="0%">
            <fmc:ToolZoomin id="zoom"/>
            <fmc:ToolZoomout left="30"/>
            <fmc:ToolPan left="60"/>
            <fmc:ToolMeasure left="90"/>
            <fmc:ToolIdentify left="120"/>
            <fmc:ButtonFull left="150" listento="map1"/>
        </fmc:ToolGroup>
        <fmc:Coordinates listento="map1" left="80%" top="3%" decimals="0">
            <style id=".xy" font-family="arial" font-size="11" />
            <string id="xy" nl="Y: [y]     X: [x]" />
        </fmc:Coordinates>
    </fmc:Container>
    <fmc:Container  borderwidth="1" bordercolor="#606060" left="0" top="7%" height="93%" width="100%">
    <fmc:BorderNavigation listento="map1" top="0%" width="100%" height="100%" skin="f1"/>
        <fmc:Map id="map1" left="0" top="0%" height="100%" width="100%" bottom="bottom" visible="true" extent="67914,352460,200359,443186" fullextent="67914,352460,200359,443186">
            <fmc:LayerIdentifyIcon  id="identifyicon"/>
        </fmc:Map>
    </fmc:Container>
</FLAMINGO>